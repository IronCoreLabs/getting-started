{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "no-prototype-builtins": 2,
        "no-template-curly-in-string": 2,
        "array-callback-return": 1,
        "block-scoped-var": 2,
        "class-methods-use-this": 2,
        "complexity": [
            2,
            10
        ],
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": 2,
        "eqeqeq": 2,
        "no-alert": "off",
        "no-caller": 2,
        "no-console": "off",
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-param-reassign": 2,
        "no-return-assign": 2,
        "no-sequences": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-useless-return": 2,
        "no-void": 2,
        "prefer-promise-reject-errors": 2,
        "no-catch-shadow": 2,
        "no-shadow": 2,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-undef-init": 2,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "callback-return": 2,
        "no-path-concat": 2,
        "arrow-parens": [
            2,
            "always"
        ],
        "arrow-spacing": 2,
        "no-confusing-arrow": 2,
        "no-duplicate-imports": 2,
        "no-useless-computed-key": 2,
        "no-useless-constructor": 2,
        "no-useless-rename": 2,
        "no-var": 2,
        "object-shorthand": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-destructuring": 2,
        "prefer-rest-params": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "rest-spread-spacing": 2,
        "template-curly-spacing": 2,
        "brace-style": [
            2,
            "stroustrup"
        ],
        "camelcase": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "func-call-spacing": 2,
        "jsx-quotes": 2,
        "key-spacing": 2,
        "linebreak-style": 2,
        "max-depth": 2,
        "max-lines": [
            2,
            450
        ],
        "max-nested-callbacks": 2,
        "max-params": [
            2,
            5
        ],
        "max-statements": [
            2,
            20
        ],
        "max-statements-per-line": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-continue": 2,
        "no-lonely-if": 2,
        "no-mixed-operators": 2,
        "no-multi-assign": 2,
        "no-multiple-empty-lines": [
            2,
            {
                "max": 1
            }
        ],
        "no-tabs": 2,
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 2,
        "no-whitespace-before-property": 2,
        "operator-assignment": 2,
        "quote-props": [
            2,
            "consistent"
        ]
    }
}
